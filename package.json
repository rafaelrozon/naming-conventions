{
    "name": "naming-conventions",
    "version": "1.0.0",
    "main": "n/a",
    "repository": "https://github.com/rafaelrozon/naming-conventions.git",
    "author": "Rafael Rozon <rafaelrozon.developer@gmail.com>",
    "license": "MIT",
    "devDependencies": {
        "format-package": "^6.1.0",
        "husky": "^4.3.0",
        "prettier": "2.1.2",
        "pretty-quick": "^3.1.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "scripts": {
        "format": "yarn prettier --write .",
        "format:pkg": "format-package -w",
        "postinstall": "yarn format:pkg"
    }
}
